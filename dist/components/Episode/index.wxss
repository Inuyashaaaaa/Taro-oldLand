.container {
  position: relative;
  display: inline-flex;
  flex-direction: row;
  align-items: baseline;
}
.plain {
  font-size: 32rpx;
}
.count {
  font-size: 60rpx;
  line-height: 60rpx;
  margin-right: 14rpx;
  font-weight: 800rpx;
}
.dateContainer {
  border-left: 1rpx solid black;
  padding-left: 16rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  bottom: 28rpx;
}
.month {
  font-size: 24rpx;
  line-height: 24rpx;
}
.year {
  font-size: 20rpx;
}